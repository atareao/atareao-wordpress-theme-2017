.center-div{
  margin: 0 auto;
}
.buttonHolder{
  text-align: center;
}
.error{
  padding: 5px 9px;
  border: 1px solid red;
  color: red;
  border-radius: 3px;
}
.success{
  padding: 5px 9px;
  border: 1px solid green;
  color: green;
  border-radius: 3px;
}
form span{
  color: red;
}
.oculto{
  visibility: hidden;
  display: none;
}
input[type="submit"],
.button,
button{
  cursor:pointer;
  background-color: #3498db;
  color:white;
  line-height: 1.6rem;
  font-size:22px;
  padding: 10px 20px;
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  transition: all 0.3s;
  border: none;
  margin: 0 auto;
}
input[type="submit"]:hover,
.button:hover,
button:hover {
  background: #CF4647;
  border: none;
}
input[type="text"],
input[type="email"],
input[type="search"],
textarea {
  font-family:'open_sansregular';
  text-indent: 45px;
  width:100%;
  color:#3c3c3c;
  font-size: 18px;
  line-height: 22px;
  background-color: #fbfbfb;
  padding: 15px 4px;
  margin-bottom: 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border: 3px solid #aaaaaa;
}
input[type="text"]:focus,
input[type="email"]:focus,
input[type="search"]:focus,
textarea:focus{
  background: #fff;
  box-shadow: 0;
  border: 3px solid #3498db;
  color: #000000;
  outline: none;
}
textarea{
  height: 120px;
}
.select{
  width: 100%;
  position: relative;
  display: inline-block;
  color:#3c3c3c;
  background-color: #fbfbfb;
  margin-bottom: 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border: 3px solid #aaaaaa;
}
select{
  font-size: 18px;
  font-weight: 400;
  font-family:'open_sansregular';
  width: 100%;
  padding: 15px 4px;
  margin-right: 7px;
  border: none;
  background-color: #fbfbfb;
  -webkit-appearance: none;
  -moz-appearance: none;
  margin-bottom: 0;
}
.select__arrow {
  position: absolute;
  top: 16px;
  right: 15px;
  width: 0;
  height: 0;
  pointer-events: none;
  border-width: 5px 5px 0 5px;
  border-style: solid;
  border-color: #7b7b7b transparent transparent transparent;
}
/*****************************************
  CHECKBOX
******************************************/
input[type="checkbox"],
input[type="radio"]{
  cursor:pointer;
}
input[type="checkbox"] {
  display: none;
}
input[type="checkbox"] + span {
  background-color: #fbfbfb;
  border: 3px solid #aaaaaa;
  display: inline-block;
  float: left;
  margin-right: 7px;
  padding: 7px;
  position: relative;
}
label:hover input[type="checkbox"] + span {
  border: 3px solid #3498db;
  background: #fff;
}
input[type="checkbox"]:checked + span:after {
  color: #aaaaaa;
  content: "\2714";
  font-size: 26px;
  left: 0px;
  position: absolute;
  top: -18px;
}
label:hover input[type="checkbox"]:checked + span {
  border: 3px solid #3498db;
  background: #fff;
}
/**/
input[type="radio"] {
  display: none;
}
input[type="radio"] + span {
  background-color: #fbfbfb;
  border: 3px solid #aaaaaa;
  border-radius: 60px;
  display: inline-block;
  float: left;
  margin-right: 8px;
  padding: 8px;
  position: relative;
  -webkit-appearance: none;
}
label input[type="radio"] + span:after {
  content: " ";
}
label:hover input[type="radio"] + span {
  border: 3px solid #3498db;
  background: #fff;
}
input[type="radio"]:checked + span:after {
  background: #aaaaaa;
  border-radius: 1000px;
  box-shadow: inset 2px #aaaaaa;
  content: " ";
  height: 12px;
  left: 3px;
  position: absolute;
  top: 2px;
  width: 10px;
}
label:hover input[type="radio"]:checked + span {
  border: 3px solid #3498db;
  background: #fff;
}

