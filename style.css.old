/* =Pagination
-------------------------------------------------------------- */
.pagination{
position:relative;
display: inline-block;
line-height:13px
}
.pagination span,.pagination a{
display:block;
float:left;
margin:5px 5px 5px 5px;
padding:12px 14px;
text-decoration:none;
width:auto;
color:#fff;
background:#555
}
.pagination a:hover{
color:#fff;
background:#3279bb
}
.pagination .current{
background:#3279bb;
color:#fff
}
.paging-navigation{
margin:16px 10px;
padding:1rem;
text-align: center;
}
.screen-reader-text{
display:none
}
/* =CSS Menu
-------------------------------------------------------------- */

#cssmenu, #cssmenu ul{
  margin: 0;
  background-color: rgb(219, 73, 20);
}
#cssmenu ul{
  padding: 0;
}
#cssmenu li{
  float: left;
  list-style-type: none;
}
#cssmenu .active a{
  border-top-style: solid;
  border-top-color: black;
  padding-top: 3px;
}

#cssmenu a{
  color: white;
  text-transform: uppercase;
}
#cssmenu a:hover{
  color:black;
}
#cssmenu li a{
  margin-left: 15px;
}
#cssmenu li .for-desktop{
  margin-left: 5px;
}

/* =Cookies
-------------------------------------------------------------- */
.cookie-policy{
display:block;
background-color:rgba(51,51,51,0.9);
color:white;
padding:0 30px 0 10px;
position:fixed;
bottom:0;
left:0;
width:100%;
font-size:13px;
z-index:999
}
.cookie-policy.acepted{
display:none
}
.cookie-policy-wrapper{
margin:0 auto;
position:relative;
text-align:left
}
.cookie-policy .link-cta{
font-size:16px;
color:white;
background-size:16px;
float:right;
margin-right:40px;
padding-bottom:10px
}
.cookie-policy-wrapper>a{
text-decoration:none
}
.cookie-policy-wrapper>a::before{
font-size:22px;
content:'\00d7'
}
/* =disqus
-------------------------------------------------------------- */
.dsq-widget-list{
padding-left:0
}
.post-message p,.dsq-widget-user,.dsq-widget-comment p{
font-size:18px;
font-size:1.125rem
}

